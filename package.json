{
  "name": "logger-multi-db",
  "version": "1.0.1",
  "description": "Universal logging library for Node.js - Works with MongoDB, PostgreSQL, MySQL, Firebase, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters/mongodb": {
      "import": "./dist/adapters/mongodb.js",
      "types": "./dist/adapters/mongodb.d.ts"
    },
    "./adapters/postgresql": {
      "import": "./dist/adapters/postgresql.js",
      "types": "./dist/adapters/postgresql.d.ts"
    },
    "./adapters/mysql": {
      "import": "./dist/adapters/mysql.js",
      "types": "./dist/adapters/mysql.d.ts"
    },
    "./adapters/firebase": {
      "import": "./dist/adapters/firebase.js",
      "types": "./dist/adapters/firebase.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logger",
    "logging",
    "mongodb",
    "postgresql",
    "mysql",
    "firebase",
    "firestore",
    "universal",
    "typescript",
    "javascript",
    "esm",
    "modules",
    "nodejs",
    "database",
    "sql",
    "nosql",
    "error-tracking",
    "monitoring"
  ],
  "author": "Jordan (tech-converter)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jordane9999/logger-multi-db"
  },
  "bugs": {
    "url": "https://github.com/Jordane9999/logger-multi-db/issues"
  },
  "homepage": "https://github.com/Jordane9999/logger-multi-db#readme",
  "peerDependenciesMeta": {
    "mongodb": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "firebase-admin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "firebase-admin": "^13.6.0",
    "mongodb": "^7.0.0",
    "mysql2": "^3.15.3",
    "mysql2-promise": "^0.1.4",
    "pg": "^8.16.3"
  }
}
